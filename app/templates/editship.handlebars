<!DOCTYPE html>
<div class="modal fade new-ship-modal-lg" tabindex="-1" role="dialog" aria-labelledby="editShipHeader" aria-hidden="true">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">Ã—</span>
        </button>
        <h4 class="modal-title" id="newShipHeader">Edit {{this.name}}</h4>
      </div>
      <div class="modal-body">
        <form id="new-ship-form">
        {{#each this}}
          {{#if this}}
            <input class="new-ship-input" name="{{@key}}" type="text" id="{{@key}}" placeholder="{{this}}" value="{{this}}">
          {{else}}
            <input class="new-ship-input" name="{{@key}}" type="text" id="{{@key}}" placeholder="{{@key}}">
          {{/if}}
        {{/each}}
        </form>
     </div>
     <div class="modal-footer">
       <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
       <button id="post-new-ship" type="button" class="btn btn-primary">Edit {{this.name}}</button>
     </div>
    </div>
  </div>
</div>
